// Init
LDI r7 0b00100000
LDI r8 0b01001110
LDI r9 0b11010111

// Manipulations
XOR r14 r8 r9   // r14 ← 10011001 = 01001110 ^ 11010111
SUB r15 r14 r7  // r15 ← 01111001 = 10011001 - 00100000
AND r13 r14 r15 // r13 ← 00011001 = 10011001 & 01111001
NOR r11 r15 r13 // r11 ← 10000110 = !01111001 = !(01111001 | 00011001)
RSH r11 r11     // r11 ← 01000011 = 10000110 >> 1
ADD r10 r8 r11  // r10 ← 10010001 = 01001110 + 01000011
ADD r10 r10 r10 // r10 ← 00100010 = 10010001 + 10010001
RSH r8 r10      // r8  ← 00010001 = 00100010 >> 1
NOR r8 r8 r8    // r8  ← 11101110 = !00010001 = !(00010001 | 00010001)
RSH r7 r11      // r7  ← 00100001 = 01000011 >> 1
HLT

// Final state
// r7 =  00100001
// r8 =  11101110
// r9 =  11010111
// r10 = 00100010
// r11 = 01000011
// r12 = 00000000
// r13 = 00011001
// r14 = 10011001
// r15 = 01111001